		$('#product_checkbox').on('click', function(){
			if ($('#product_checkbox').is(':checked')){
				 $('#datatable_fixed_column td input[type="checkbox"]').prop('checked', true);
			}else{
				 $('#datatable_fixed_column td input[type="checkbox"]').prop('checked', false);
			}
				 
		  });
			
			   // Handle click on checkbox to set state of "Select all" control
			  
		
	});
	
