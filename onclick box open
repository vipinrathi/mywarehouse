<script type="text/javascript">
		
	// DO NOT REMOVE : GLOBAL FUNCTIONS!
		
	$(document).ready(function() {
		
		pageSetUp();
		//var responsiveHelper_datatable_fixed_column = undefined;
		var breakpointDefinition = {
			tablet : 1024,
			phone : 480
		};
		var rows_selected = [];
		
		
		
		var otable = $('#datatable_fixed_column').DataTable({
												  
			"processing": true,
			"pageLength" : 20,
			"serverSide": true,
			"ajax":{
					url	:"index.php?route=account/customerpartner/productlist/products", 					
					// json datasource
					type	: "post",  // type of method  ,GET/POST/DELETE
					data	: function(d){
							d.mode_id='<?php echo $mode; ?>';
							d.filter_name = $('input[name=\'filter_name\']').val();
							d.filter_sku = $('input[name=\'filter_sku\']').val();
							d.filter_price = $('input[name=\'filter_price\']').val();
							d.filter_upc = $('input[name=\'filter_upc\']').val();
							d.filter_group = $('select[name=\'filter_group\']').val();
							d.filter_category_id = $('select[name=\'filter_category_id\']').val();
						   },
					error: function(){
								$("#datatable_fixed_column").css("display","none");
							}
					},
			"deferLoading": 0,
			"columns":	[

							{data: 'product_id', class: 'center1'},
							{ data: 'product_id', class: 'center1'} ,

							{ data: 'name' },
							{ data: 'sku' },
							{ data: 'upc', class: 'right1' },
							{ data: 'quantity', class: 'right1' },
							{ data: 'price', class: 'right1' },
							{ data: 'sales', class: 'right1' },
							{ data: 'parentage', class: 'center1' },
							<?php if($mode == 2 || $mode == 3) { ?>
							
							{ data: 'fulfilled_by', class: 'center1' },
							
							<?php }  ?>
							
							
							
							{ data: 'action', class: 'center1' }
						]	
								,
			"columnDefs": [
						
							{
								'targets': 0,
								'searchable':false,
								'orderable':false,

								//'className':'dt-body-center',
								'render': function (data, type, row, full, meta){
								   return '<input type="checkbox" name="select[]" id="product_checkbox" value="'+ data +'">';
								 }

							 },
			
							{
								
								"render": function ( data, type, row ) {
									if(!data || data =="" )
									return "<b>Currency not available</b>" ;
									else
									return data;	
								},
								"targets": 2
							},
							
							{
								
								"render": function ( data, type, row ) {
									if(!data || data =="")
									return "<b>Name not available</b>" ;
									else
									return data;	
								},
								"targets": 4
											
							},
							{
								"render": function ( data, type, row ) {
									if(!data || data =="$0.00")
									return "<b>Not available</b>" ;
									else
									return data;	
								},
								"targets": 5
							},
							{
								"render": function ( data, type, row ) {
									
									return "$" + data;	
								},
								"targets": 6
							},
							{
								"render": function ( data, type, row ) {
									if(!data )
										return "<b>No Sales</b>" ;
									else 
										return "$" + data;	
								},
								"targets": 7
							},
							{
								"render": function ( data, type, row ) {
									if(!data )
										return "<b>Standalone</b>" ;
									else 
										return data;	
								},
								"targets": 8
							},
							{
								<?php if($mode == 2 || $mode == 3) { ?>
									"targets": 10,
								<?php } else {  ?>
									"targets": 9,
								<?php }   ?>
								"render": function (data, type, row) { 
									return '<a href="index.php?route=account/customerpartner/addproduct&product_id='+ row.product_id +'&mode=<?php echo $mode; ?>" class="btn btn-info" data-id="' + row.product_id + '"><i class="fa fa-pencil"/></i></a> &nbsp;&nbsp; <a href="index.php?route=account/customerpartner/productlist&product_id='+ row.product_id +'" class="btn btn-info" name="copy_product" data-id="' + row.product_id + '"><i class="fa fa-copy"></i></a>';
									
									}
											
							},
							/*{
								"targets": 7,
								
										
								"render": function (data, type, row) { 
									return '<a href="index.php?route=account/customerpartner/addproduct&product_id='+ row.product_id +'" class="btn btn-info" data-id="' + row.product_id + '"><i class="fa fa-copy"></i></a>';
									
									}
							},*/
							
						
						],
			'rowCallback': function(row, data, dataIndex){
							 // Get row ID
							 var rowId = data[0];

							 // If row ID is in the list of selected row IDs
							 if($.inArray(rowId, rows_selected) !== -1){
								$(row).find('input[type="checkbox"]').prop('checked', true);
								$(row).addClass('selected');
							 }
						},	
			"dom": "<'dt-toolbar'<'col-xs-12 col-sm-6 hidden-xs'><'col-sm-6 col-xs-12 hidden-xs'		<'toolbar'>>r>"+
						"t"+
							"<'dt-toolbar-footer'<'col-sm-6 col-xs-12 hidden-xs'i><'col-xs-12 col-sm-6'p>>",
			"autoWidth" : true
			/*,
			"preDrawCallback" : function() {
							//Initialize the responsive datatables helper once.
								if (!responsiveHelper_datatable_fixed_column) {
													
									responsiveHelper_datatable_fixed_column = new ResponsiveDatatablesHelper($('#datatable_fixed_column'), breakpointDefinition);
								}
						},
								"rowCallback" : function(nRow) { 
							responsiveHelper_datatable_fixed_column.createExpandIcon(nRow);
						},
								"drawCallback" : function(oSettings) { 
							responsiveHelper_datatable_fixed_column.respond();
						}	
			*/
						
		});   
		
		$('#product-div').hide();
		$('#button-filter').on('click', function() {
				$('#product-div').show();
				otable.ajax.reload();
				
		});
		
		// Handle click on checkbox
		/*
		$('#datatable_fixed_column tbody').on('click', 'input[type="checkbox"]', function(e){
		  var $row = $(this).closest('tr');
