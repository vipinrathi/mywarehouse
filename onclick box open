<script type="text/javascript">

	$("#marketplaces_1").click(function (){
		//alert($("#marketplaces_1:checked").length);
		//if($("#marketplaces").val()  == 1){
		if($("#marketplaces_1:checked").length > 0){	
			$('#fulfilled_1').removeAttr('disabled');
			//return false;
		}else{
			$('#fulfilled_1').attr('disabled', 'disabled');
		}
		
	});
	
	$("#marketplaces_2").click(function (){
		//alert($("#marketplaces").val());
		//if($("#marketplaces").val()  == 1){
		if($("#marketplaces_2:checked").length > 0){	
			$('#fulfilled_2').removeAttr('disabled');
			//return false;
		}else{
			$('#fulfilled_2').attr('disabled', 'disabled');
		}
		
	});	
	
	
	$("#marketplaces_all_1").click(function (){
		//alert($("#marketplaces_all_1:checked").length);
		//if($("#marketplaces").val()  == 1){
		if($("#marketplaces_all_1:checked").length > 0){	
			$('#fulfilled_all_1').removeAttr('disabled');
			//return false;
		}else{
			$('#fulfilled_all_1').attr('disabled', 'disabled');
		}
		
	});
	
	$("#marketplaces_all_2").click(function (){
		//alert($("#marketplaces").val());
		//if($("#marketplaces").val()  == 1){
		if($("#marketplaces_all_2:checked").length > 0){	
			$('#fulfilled_all_2').removeAttr('disabled');
			//return false;
		}else{
			$('#fulfilled_all_2').attr('disabled', 'disabled');
		}
		
	});

		
	
	$("#assign_mp").click(function (){
		//alert($("#marketplaces_1:checked").length  == 0);
		//alert($("#marketplaces_2:checked").length  == 0);
		if($("#product_checkbox:checked").length  == 0){
			$(".err1").css("display", "block");
			return false;
		}else{
			$(".err1").css("display", "none");
		}
		if(($("#marketplaces_1:checked").length  == 0) && ($("#marketplaces_2:checked").length  == 0)){
		$(".err2").css("display", "block");
		return false;
		}else{
			//alert("hello");
			$(".err2").css("display", "none");
			var ss=$("select[id=fulfilled_1]").val();
			var s2=$("select[id=fulfilled_2]").val();
			if(( ss == 0) && (s2 == 0)){
				$(".err2_1").css("display", "block");

				return false;			
			}else{
				if(($("#marketplaces_1:checked").length > 0) && ( ss == 0)){

					$(".err2_1_1").css("display", "block");
					return false;
				}else{
					$(".err2_1_1").css("display", "none");

				}

				if(($("#marketplaces_2:checked").length > 0) && ( s2 == 0)){

					$(".err2_1_2").css("display", "block");
					return false;
				}else{
					$(".err2_1_2").css("display", "none");

				}
				
				

				$(".err2_1").css("display", "none");
			}

		}
		return true;
	});
	
	$("#assign_wh").click(function (){
		
		if($("#product_checkbox:checked").length  == 0){
			$(".err3").css("display", "block");
			return false;
		}else{
			$(".err3").css("display", "none");
		}
		if($("#warehouses:checked").length  == 0){
			$(".err4").css("display", "block");
			return false;
		}else{
			$(".err4").css("display", "none");
		}
		return true;
	});
	
	$("#assign_wh_all").click(function (){
		
		if($("#warehouses:checked").length  == 0){
			$(".err4").css("display", "block");
			return false;
		}else{
			$(".err4").css("display", "none");
		}
		return true;
	});
	
	$("#assign_mp_all").click(function (){
		
		if(($("#marketplaces_all_1:checked").length  == 0) && ($("#marketplaces_all_2:checked").length  == 0)){
		$(".err2").css("display", "block");
		return false;
		}else{
			//alert("hello");
			$(".err2").css("display", "none");
			var ss=$("select[id=fulfilled_all_1]").val();
			var s2=$("select[id=fulfilled_all_2]").val();
			if(( ss == 0) && (s2 == 0)){
				$(".err2_1").css("display", "block");

				return false;			
			}else{
				if(($("#marketplaces_all_1:checked").length > 0) && ( ss == 0)){

					$(".err2_1_1").css("display", "block");
					return false;
				}else{
					$(".err2_1_1").css("display", "none");

				}

				if(($("#marketplaces_all_2:checked").length > 0) && ( s2 == 0)){

					$(".err2_1_2").css("display", "block");
					return false;
				}else{
					$(".err2_1_2").css("display", "none");

				}
				
				

				$(".err2_1").css("display", "none");
			}

		}
		return true;
	});
	
	$("#show_new_group").click(function (){
		
		$("#show_new_group").css("display", "none");
		$("#group_crate_section").css("display", "block");
		$('button[name="prd_to_group"]').prop('disabled', true);
		$("input[id='groups']:checkbox").prop('checked', false);
		$("input[id='groups']:checkbox").prop('disabled', true);
		
	});
	
	$("#hide_new_group").click(function (){
		
		$("#show_new_group").css("display", "block");
		$("#group_crate_section").css("display", "none");
		$('button[name="prd_to_group"]').prop('disabled', false);
		$("input[id='groups']:checkbox").prop('disabled', false);
		
	});
	
	$("#add_new_group").click(function (){
		
		if($(".g_text").val()  == ''){
			$(".err5").css("display", "block");
			return false;
		}else{
			$(".err5").css("display", "none");
		}
		
		if($("#product_checkbox:checked").length  == 0){
			$(".err6").css("display", "block");
			return false;
		}else{
			$(".err6").css("display", "none");
		}
		return true;
	});
	
	$("#prd_to_group").click(function (){
		
		if($("#groups:checked").length  == 0){
			$(".err7").css("display", "block");
			return false;
		}else{
			$(".err7").css("display", "none");
		}
		
		if($("#product_checkbox:checked").length  == 0){
			$(".err8").css("display", "block");
			return false;
		}else{
			$(".err8").css("display", "none");
		}
		return true;
	});
	
</script>

